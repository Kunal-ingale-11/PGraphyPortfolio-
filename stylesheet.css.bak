*
{
	margin:0;
	padding:0;
	box-sizing:border-box;
	
}
body
	{
		background-image:url(camera.jpg);
		font-family:Arial;
		background-size:cover;
		left:50px;
		margin:100px;
		margin-left:275px;
				
	}
.active
{
	background-color:#ff9933;
	border:1px solid white;
	
}
ul
{
	top:100px;
	background-color:#ff9933;

	list-style:none;
	opacity:1;
}
ul li
{
	float:left;
	
	width:150px;
	padding:0px;
	height:30px;
		
	opacity:1;
	line-height:30px;
	text-align:center;
	font-size:15px;
	
}

ul li a{
	text-decoration:none;
	color:white;
	display:block;
	border:1px;
}
ul li a:hover
{
	background-color:#ff9933;
	border:1px solid white;
	border-bottom:1px double #fff;
	border-radius:1px;
	
}
ul li ul li{
	display:none;
}
ul li:hover ul li{
	display:block;
	opacity:1;
	border-bottom:1px;
}
